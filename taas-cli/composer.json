{
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "symfony/config": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/process": "^6.0",
        "symfony/yaml": "^6.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Taas\\Cli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Taas\\Cli\\Tests\\": "tests/"
        }
    }
}
